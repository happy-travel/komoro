{
  "AllowedHosts": "*",
  "Authority": {
    "Options": "komoro/authority"
  },
  "Database": {
    "ConnectionString": "Server={0};Port={1};Database=komoro;Userid={2};Password={3};",
    "Options": "komoro/connection-string"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "HappyTravel": "Information"
    }
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Telemetry": {
    "IsEnabled": false
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "komoro",
    "Token": "HTDC_VAULT_TOKEN"
  }
}
